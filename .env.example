# Sweek-AI Environment Configuration
# Copy this file to .env and update with your values

# Application Settings
WEBUI_NAME=Sweek-AI
WEBUI_URL=http://localhost:3000
WEBUI_SECRET_KEY=change-this-to-a-random-secret-key-in-production

# Database Settings
POSTGRES_DB=sweekdb
POSTGRES_USER=sweekuser
POSTGRES_PASSWORD=change-this-password-in-production
DATABASE_URL=postgresql://sweekuser:change-this-password-in-production@postgres:5432/sweekdb

# Ollama Settings
OLLAMA_HOST=http://ollama:11434
OLLAMA_KEEP_ALIVE=24h

# Model Settings
DEFAULT_MODELS=llama3.2:1b,llama3.1:8b,codellama:13b
MODEL_FILTER_LIST=llama3.2:1b,llama3.1:8b,codellama:13b,einstein,minecraft-steve,code-mentor

# Feature Flags
ENABLE_SIGNUP=true
ENABLE_COMMUNITY_SHARING=false
ENABLE_MODEL_FILTER=true
ENABLE_ADMIN_EXPORT=true
ENABLE_ADMIN_CHAT_ACCESS=true

# Security Settings
WEBUI_AUTH=true
DEFAULT_USER_ROLE=user
ENABLE_OAUTH=false

# Redis Settings
REDIS_URL=redis://redis:6379/0

# Development Settings
DEBUG=false
LOG_LEVEL=info
# WHITE-LABEL SETTINGS
DEFAULT_USER_THEME=dark
WEBUI_DEFAULT_THEME=dark
WEBUI_LOGO_URL=/static/logos/sweek-ai-logo.svg
WEBUI_LOGO_DARK_URL=/static/logos/sweek-ai-logo-dark.svg
WEBUI_FAVICON_URL=/static/logos/favicon.ico

# WHITE-LABEL SETTINGS
DEFAULT_USER_THEME=dark
WEBUI_DEFAULT_THEME=dark
WEBUI_LOGO_URL=/static/logos/sweek-ai-logo.svg
